# Install the default version
COPY --chown=<%= chown %> images/<%= analyzer %>/Pipfile ${RUNNER_USER_HOME}/<%= analyzer %>_Pipfile
RUN cd "${RUNNER_USER_HOME}" && \
    mkdir <%= analyzer %> && \
    mv <%= analyzer %>_Pipfile <%= analyzer %>/Pipfile && \
    cd <%= analyzer %> && \
    pipenv install --system --skip-lock
