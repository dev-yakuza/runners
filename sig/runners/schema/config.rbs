module Runners
  module Schema
    class BaseConfigClass < StrongJSON
      def base: () -> StrongJSON::Type::Object[untyped]
      def git: () -> StrongJSON::Type::Enum[untyped]
      def ruby: () -> StrongJSON::Type::Object[untyped]
      def cplusplus: () -> StrongJSON::Type::Object[untyped]
      def npm_install: () -> StrongJSON::Type::Enum[untyped]
      def npm: () -> StrongJSON::Type::Object[untyped]
      def java: () -> StrongJSON::Type::Object[untyped]
    end
    BaseConfig: BaseConfigClass

    class ConfigClass < StrongJSON
      def payload: () -> StrongJSON::Type::Object[untyped]
      def register: (name: Symbol, schema: StrongJSON::_Schema[untyped]) -> void
    end
    Config: ConfigClass
  end
end
